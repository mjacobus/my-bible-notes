<%= render breadcrumb  %>
<%= render flash  %>

<%= simple_form_for(timeline, url: url, as: :timeline) do |f| %>
  <%= input_wrapper do %>
    <%= f.input :name %>
  <% end %>
  <%= input_wrapper do %>
    <%= f.input :slug %>
  <% end %>
  <%= input_wrapper do %>
    <%= f.input :description, as: :text %>
  <% end %>
  <%= input_wrapper do %>
    <%= f.submit(class: 'btn btn-primary mt-3 float-right') %>
  <% end %>
<% end %>
